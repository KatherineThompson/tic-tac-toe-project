{
  "name": "tic-tac-toe",
  "version": "1.0.0-0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "jshint": "^2.9.1",
    "node-sass": "^3.4.1",
    "node-static": "^0.7.7"
  },
  "scripts": {
    "start": "static -a 0.0.0.0",
    "test": "jshint -c .jshintrc index.js && npm run build",
    "build": "babel index.js --out-file built.js && node-sass index.scss built.css",
    "build-styles-watch": "node-sass --watch index.scss built.css"
  }
}
